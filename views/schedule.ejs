<!-- aggrigate events page. primary only -->

<!DOCTYPE html>
<html lang="en">

<head>
  <title>Aggrigate Schedules Page</title>

  <%- include('_partials/head') %>

  <script src="/scripts/schedule_FE.js"></script>

</head>


<body>

  <%- include('_partials/nav') %>

  <main>

    <section id="middle-bar">

      <div class="schedules-container">
        <table class="table table-dark table-striped-columns">
          <thead>
            <tr>
              <th scope="col"> URL </th>
              <th scope="col"> Range </th>
              <th scope="col"> Current Participants </th>
              <th scope="col"> Event </th>
              <th scope="col"> </th>
            </tr>
          </thead>
          <tbody class="table-group-divider">
            <% for (let schedule of schedules) { %>
              <tr data-id="<%= schedule.id %>" name="container">

                <td> secondary/<%= schedule.url %></td>
                <td><%= schedule.range %>  </td>
                <td><%= schedule.count %></td>
                <td><%= schedule.type %></td>

                <td><a class="btn btn-outline-light" href="primary/<%= schedule.url %>">
                  results
                </a></td>
              </tr>
            <% } %>
          </tbody>
        </table>

      </div>
    </section>

  </main>
</body>

</html>
